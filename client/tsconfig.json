{
  "compilerOptions": {
		"baseUrl": ".",
		"target": "es5",
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "paths": {
      "@/*": ["./src/*"],
      "@types/*": ["./src/types/*"],
      "@redux/*": ["./src/redux/*"],
      "@hooks/*": ["./src/types/*"],
      "@components/*": ["./src/components/*"],
      "@assets/*": ["./src/assets/*"]
    }
	},
	"exclude": ["node_modules", "dist"] 
}
